{
    "manifest_version": 3,
    "name": "D2L+ Canary",
    "version": "1.0",
    "background": {
      "service_worker": "dist/background.js"
    },
    "permissions": [
      "activeTab",
      "tabs"
    ],
    "web_accessible_resources": [
      {
        "resources": ["preview.html"],
        "matches": [ "*://*/*" ]
      },
      {
        "resources": ["src/lz-string-default.min.js"],
        "matches": [ "*://*/*" ]
      }
    ],
    "content_scripts": [
      {
        "matches":["*://d2l.msu.edu/*"],
        "js": ["dist/contentScript.js"],
        "run_at": "document_end"
      }
    ],
  "host_permissions": [
    "*://*.d2l.msu.edu//"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}

  